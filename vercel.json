{
  "version": 2,
  "builds": [
    {
      "src": "matrix-scaffold/backend/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/matrix-scaffold/backend/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["us-east-1", "eu-west-1", "ap-southeast-1"]
}

